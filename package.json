{
  "name": "Kokua",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-sim": "react-native run-ios --simulator='iPhone 14 Pro Max'",
    "start": "react-native start",
    "start-reset-cache": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "ios-release": "react-native run-ios --configuration=release",
    "android-release": "cd android && ./gradlew assembleRelease && cd ..",
    "nodeify": "rn-nodeify --hack --install crypto,stream && rm -f package-lock.json shim.js",
    "postinstall": "yarn nodeify"
  },
  "dependencies": {
    "@freakycoder/react-native-bounceable": "^1.0.3",
    "@noble/ed25519": "^1.7.1",
    "@noble/hashes": "^1.1.3",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/checkbox": "^0.5.12",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@sentry/react-native": "^4.5.0",
    "@stripe/stripe-identity-react-native": "^0.1.1",
    "@types/jest": "^29.0.3",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-auth": "^1.0.0",
    "babel-jest": "^29.0.3",
    "big-integer": "^1.6.51",
    "bigint-polyfill": "^0.1.0",
    "cross-fetch": "^3.1.5",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.2",
    "jest-environment-jsdom": "^29.1.2",
    "react": "18.1.0",
    "react-native": "0.70.1",
    "react-native-bootsplash": "^4.3.2",
    "react-native-config": "^1.4.6",
    "react-native-crypto": "^2.2.0",
    "react-native-device-info": "^10.3.0",
    "react-native-dotenv": "^3.3.1",
    "react-native-drop-shadow": "^0.0.5",
    "react-native-gesture-handler": "^2.7.1",
    "react-native-image-header-scroll-view": "^1.0.0",
    "react-native-keychain": "^8.1.1",
    "react-native-numeric-input": "^1.9.1",
    "react-native-paper": "^5.0.0-rc.6",
    "react-native-randombytes": "^3.6.1",
    "react-native-reanimated": "^2.10.0",
    "react-native-reanimated-carousel": "^3.0.6",
    "react-native-safe-area-context": "^4.4.0",
    "react-native-screens": "^3.17.0",
    "react-native-select-dropdown": "^3.0.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.2.0",
    "react-native-swiper-flatlist": "^3.0.17",
    "readable-stream": "^1.0.33",
    "stream-browserify": "^1.0.0",
    "text-encoding-polyfill": "^0.6.7",
    "urql": "^3.0.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.26",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.13",
    "@testing-library/react-native": "^11.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/react-native": "^0.70.0",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "jest": "^29.0.0",
    "metro-react-native-babel-preset": "^0.72.1",
    "react-dom": "18.1.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.1.0",
    "rn-nodeify": "^10.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./jestSetupFile.js",
      "core-js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@sentry|@react-native|@react-navigation|react-native)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  },
  "browser": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  }
}
